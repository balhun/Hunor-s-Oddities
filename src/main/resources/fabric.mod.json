{
	"schemaVersion": 1,
	"id": "hunors-oddities",
	"version": "${version}",
	"name": "Hunor's Oddities",
	"description": "This mod is about basically anything my mind can come up with.",
	"authors": [
		"Hunor Balazs"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "MIT",
	"icon": "assets/hunors-oddities/icon.png",
